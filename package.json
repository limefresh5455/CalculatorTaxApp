{
  "name": "tax_calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx --ignore-path ./.gitignore src",
    "lint_server": "eslint -c ./server/.eslintrc.json --ext .ts --ignore-path ./server/.gitignore server",
    "format_check": "prettier --check --ignore-path ./.gitignore .",
    "format": "prettier --write --ignore-path ./.gitignore .",
    "validate": "npm run lint && npm run lint_server && npm run format_check && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "clsx": "^2.0.0",
    "dotenv": "^16.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-datasheet-grid": "^4.11.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "prettier": "2.8.4",
    "vite": "^4.1.0"
  }
}
